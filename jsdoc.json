{
  "source": {
    "include": ["./js/"],
    "includePattern": "\\.js$",
    "exclude": ["node_modules/", "tests/", "scripts/"]
  },
  "opts": {
    "destination": "./docs/",
    "recurse": true
  },
  "plugins": [
    "plugins/markdown"
  ],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "markdown": {
    "parser": "gfm",
    "hardwrap": true
  },
  "metadata": {
    "title": "Whisper Transcribe Documentation",
    "description": "Speech-to-Text Transcription App using Azure Speech Services"
  }
}
